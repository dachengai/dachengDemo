<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dacheng.dachengdemo" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name=".base.app.DemoApplication">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".dialog.DialogAct"/>
        <activity android:name=".recyclerview.RecyclerViewAct"/>
        <activity android:name=".searchBar.SearchViewActivity"/>
        <activity android:name=".qrcode.QRCodeAct">
        </activity>
        <activity
            android:name=".qrcode.QRCodeScanAct"
            android:screenOrientation="fullSensor"
            android:stateNotNeeded="true"
            android:theme="@style/zxing_CaptureTheme"
            android:windowSoftInputMode="stateAlwaysHidden">

        </activity>
        <activity android:name=".customview.progress.ProgressAct"/>
        <activity android:name=".base.image.picasso.ImageAct"/>
        <activity android:name=".mvp.login.LoginActivity"/>
        <activity android:name=".service.ServiceAct"/>
        <service android:name=".service.MyService"/>
        <activity android:name=".customview.autoscrolltitle.ScrollTitleAct"/>
        <activity android:name=".customview.CustomAct"/>
        <activity android:name=".listview.ListViewAct"/>
        <activity android:name=".customview.imagecoverflow.CoverFlowViewAct"/>
        <activity android:name=".customview.imagescroll.ScrollImageAct"/>
    </application>

</manifest>
